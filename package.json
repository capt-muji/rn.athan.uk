{
  "name": "athan",
  "version": "1.0.27",
  "type": "commonjs",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start -c",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "reset": "yarn clean && yarn install && yarn husky && yarn start",
    "clean": "rm -rf ios android node_modules yarn.lock .expo .vscode .husky && yarn cache clean && rm -rf /tmp/metro-*",
    "add": "echo 'Use: `npx expo install <package>` for Expo/RN packages, yarn add <package> for others'",
    "husky": "husky install && husky add .husky/pre-commit 'npx lint-staged'"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,mjs}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "5.2.8",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "expo": "52.0.48",
    "expo-av": "15.1.7",
    "expo-background-fetch": "13.1.6",
    "expo-constants": "17.1.8",
    "expo-dev-client": "~5.0.20",
    "expo-font": "13.3.2",
    "expo-haptics": "14.1.4",
    "expo-intent-launcher": "12.1.5",
    "expo-linear-gradient": "14.1.5",
    "expo-linking": "7.1.7",
    "expo-notifications": "0.29.14",
    "expo-router": "4.0.22",
    "expo-splash-screen": "0.29.24",
    "expo-system-ui": "4.0.9",
    "expo-task-manager": "12.0.6",
    "expo-updates": "0.26.19",
    "jotai": "2.16.1",
    "react": "18.3.1",
    "react-native": "0.77.3",
    "react-native-edge-to-edge": "1.7.0",
    "react-native-gesture-handler": "2.30.0",
    "react-native-mmkv": "3.3.3",
    "react-native-pager-view": "6.9.1",
    "react-native-permissions": "5.4.4",
    "react-native-reanimated": "4.0.0-beta.2",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "4.10.0",
    "react-native-svg": "15.12.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@types/node": "22.19.3",
    "@types/react": "18.3.27",
    "eslint": "9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-unused-imports": "4.3.0",
    "husky": "8.0.3",
    "lint-staged": "15.5.2",
    "pino": "9.14.0",
    "pino-pretty": "13.1.3",
    "prettier": "3.7.4",
    "react-native-svg-transformer": "1.5.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0"
  }
}
